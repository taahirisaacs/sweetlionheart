{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "composer", "roots", "wordpress", "wp", "wp-config"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/yangm97/elementor-pro.git"
    },
    {
      "type": "vcs",
      "url": "https://taahirisaac@github.com/taahirisaacs/gappointments-plugin.git"
    },
    {
      "type": "vcs",
      "url": "https://taahirisaac@github.com/taahirisaacs/woocommerce-gravityforms-product-addons-master.git"
    },
    {
      "type": "vcs",
      "url": "https://taahirisaacs@github.com/taahirisaacs/gravity-forms-plugin.git"
    }
  ],
  "require": {
    "php": ">=7.1",
    "composer/installers": "^1.8",
    "vlucas/phpdotenv": "^4.1.3",
    "oscarotero/env": "^1.2.0",
    "roots/wordpress": "5.4.1",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "wpackagist-plugin/seo-by-rank-math":">=1.0.42.1",
    "wpackagist-plugin/mailchimp":">=1.5.7",
    "wpackagist-theme/hello-elementor":">=2.2.2",
    "wpackagist-plugin/elementor":">=2.9.7",
    "wpackagist-plugin/woocommerce":">=4.1.0",
    "wpackagist-plugin/password-protected":"2.2.5",
    "yangm97/elementor-pro":">=2.9.3",
    "taahirisaacs/gappointments-plugin":">=1.9.3",
    "taahirisaacs/gravity-forms-plugin":">=2.4.18.2",
    "taahirisaacs/woocommerce-gravityforms-product-addons-master":">=3.3.10",
    "gotoandplay/gravityforms-composer-installer":"^2.0",
    "wpackagist-plugin/powerpack-lite-for-elementor":"1.2.9.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5.4",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}
