    "wpackagist-plugin/webappick-product-feed-for-woocommerce":"4.0.7",
    "wpackagist-plugin/leadin":"7.45.6",
    "wpackagist-plugin/code-snippets":">=2.14.0",
    "wpackagist-plugin/ele-custom-skin":"3.0.0",
    "wpackagist-plugin/woocommerce-advanced-free-shipping":"1.1.4",
    "wpackagist-plugin/conditional-shipping-for-woocommerce":">=2.2.0",
    "fluxfullcircle/gf-hubspot":"1.5.1",
    "wpackagist-plugin/flexible-product-fields":"1.7.5",
    "taahirisaacs/flexible-product-fields-pro":"1.8.3",
    "taahirisaacs/gappointments-plugin":">=1.9.3",
    "taahirisaacs/elementor-pro":">=2.10.3",
    "taahirisaacs/gravity-forms-plugin":">=2.4.18.2",
    "taahirisaacs/gf-zapier":">=3.3",
    "taahirisaacs/gf-bulk-actions":">=1.2.27",
    "taahirisaacs/woocommerce-gateway-snapscan":">=1.1.10",
    "taahirisaacs/acf-pro":">=5.8.12",
    "taahirisaacs/snapscan":"1.4",
    "taahirisaacs/delivery-slots-premium":">=1.14.0",
    "taahirisaacs/woo-conditional-shipping-pro":"dev-master",
    "taahirisaacs/woocommerce-zapier":"2.2.2",
    "wpackagist-plugin/jetpack":">=10.4-a.1",
    "wpackagist-plugin/woo-checkout-field-editor-pro":">=1.5.0",
    "taahirisaacs/gf-image-choices":">=1.3.45"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5.4",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}
